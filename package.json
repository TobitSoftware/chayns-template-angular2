{
  "name": "chayns-template-angular",
  "version": "0.1.1",
  "description": "A basic template for Angular based chayns tapps",
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "build": "npm run clean && webpack --config config/webpack.prod.js --progress --profile --bail",
    "deploy": "npm run clean && npm run lint && webpack --config config/webpack.prod.dp.js --progress --profile --bail",
    "lint": "tslint -c tslint.json -p src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TobitSoftware/chayns-template-angular2.git"
  },
  "keywords": [
    "chayns",
    "tobit",
    "angular",
    "typescript",
    "tapp"
  ],
  "author": "Lukas Kolletzki <lukas.kolletzki@tobit.software>",
  "contributors": [
    "Robert Verst <robert.verst@tobit.software>",
    "Henning Kuehl <henning.kuehl@tobit.software>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TobitSoftware/chayns-template-angular2/issues"
  },
  "homepage": "https://github.com/TobitSoftware/chayns-template-angular2#readme",
  "dependencies": {
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/platform-browser": "^4.4.5",
    "core-js": "^2.5.1",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.4.5",
    "@ngtools/webpack": "^1.7.4",
    "@types/chayns": "^3.1.0",
    "@types/node": "^8.0.34",
    "codelyzer": "^3.2.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-deploy-after-build": "^1.0.5",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.0"
  }
}
